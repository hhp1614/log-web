{
  "name": "@hhp1614/log-web",
  "version": "1.1.1",
  "description": "浏览器日志类",
  "main": "dist/log-web.cjs.js",
  "module": "dist/log-web.esm.js",
  "web": "dist/log-web.min.js",
  "webName": "LogWeb",
  "typings": "dist/types/main.d.ts",
  "exports": {
    ".": {
      "node": {
        "require": "./dist/log-web.cjs.js",
        "import": "./dist/log-web.esm.js"
      },
      "default": "./dist/log-web.min.js"
    },
    "./dist/": "./dist/"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "major": "npm version major -m \"Upgrade to %s\"",
    "minor": "npm version minor -m \"Upgrade to %s\"",
    "patch": "npm version patch -m \"Upgrade to %s\"",
    "prepublishOnly": "rollup -c"
  },
  "private": false,
  "keywords": [
    "utils",
    "browser",
    "log"
  ],
  "author": "hhp1614<hhp1614@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hhp1614/log-web.git"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.0.5"
  }
}
